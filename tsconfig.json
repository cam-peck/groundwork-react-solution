{
  "compilerOptions": {
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES6",
    "lib": [
      "dom",
      "es6"
    ],
    "strict": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules",
    "es2015"
  ]
}
